# Environment Variables for Aimo Wallet
# 
# Copy this file to .env and fill in your values.
# DO NOT commit .env to version control!

# Ethereum RPC URLs
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR-PROJECT-ID
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR-PROJECT-ID

# Optional: Other networks
POLYGON_RPC_URL=https://polygon-rpc.com
BSC_RPC_URL=https://bsc-dataseed.binance.org
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# 0x Swap API Key (required for swap functionality)
# Get your API key from: https://0x.org/docs/introduction/getting-started
# 0x API v2 uses permit2 for secure approvals
ZERO_X_API_KEY=e60b82af-e6fa-4132-969e-efe1d43cd4bb

# Optional: Analytics and Monitoring
# SENTRY_DSN=https://...
# FIREBASE_API_KEY=...

# Optional: Feature Flags
# ENABLE_BIOMETRIC=true
# ENABLE_ROOT_DETECTION=true
# ENABLE_SCREENSHOT_PROTECTION=true
